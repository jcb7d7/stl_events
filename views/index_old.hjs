	<script>
		var mySlider, i=0;
		function doOnLoad(){
			mySlider = new dhtmlXSlider({
				parent: "sliderObj",
				linkTo: ["sliderLink", "sliderLink2"],
				step: 1,
				min: 0,
				max: 13,
				value: [0, 6],
				range: true
			});

			mySlider.attachEvent("onSlideEnd", function(value){
				var eg = [];
			        var eventGroup = document.forms['demoForm'].elements[ 'eventGroup[]' ];
				for ( var i = 0; i < eventGroup.length; i++) {
				  if (eventGroup[i].checked) {
				    eg.push(eventGroup[i].value);
				  }
				}
				setNoteToLog({
					eventNme: "onSlideEnd",
					arg: [value],
					eg_radio: [eg]
				});
			});
		};

		function doOnUnload(){
			if (mySlider != null){
				mySlider.unload();
				mySlider = null;
			}
		};

		function setNoteToLog(data) {
            	   $.post('/', data, function (data) {
			markers = [];
			infoWindowContent = [];
			//console.log(data);
			for (var i = 0; i < data.length; i++) {
                           markers.push([data[i].name, data[i].lat, data[i].lon]);
			   if (data[i].date.includes("T")) {
                              mydate = data[i].date.split("T")[0];
			   } else {
			      mydate = data[i].date;
			   }
			   var mystr = "<h6>" + data[i].name + " <br/>" + mydate + " - " +
				data[i].time + "<br/> <a style='color:green' href=" + data[i].link + ">" +
			        data[i].link + "</a> </h6>";
			 //  console.log(mystr);
			   infoWindowContent.push([mystr]);
                        }
			google.maps.event.trigger(map, 'resize');
                        initialize();
                        });
			/*var xhr = new XMLHttpRequest();
			xhr.open("POST", '/', true);
			xhr.setRequestHeader('Content-Type', 'application/json');
			xhr.send(data);*/
			//console.log(data);
		};

	</script>

